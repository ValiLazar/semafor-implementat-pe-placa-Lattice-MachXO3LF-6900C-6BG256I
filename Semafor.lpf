BLOCK RESETPATHS;
BLOCK ASYNCPATHS;

LOCATE COMP "clk" SITE "C8" ;
FREQUENCY PORT "clk" 12.000000 MHz ;
IOBUF PORT "clk" PULLMODE=NONE IO_TYPE=LVCMOS33 ;
LOCATE COMP "buton" SITE "B3" ;
IOBUF PORT "buton" PULLMODE=UP IO_TYPE=LVCMOS33 ;

LOCATE COMP "red" SITE "H11" ;
