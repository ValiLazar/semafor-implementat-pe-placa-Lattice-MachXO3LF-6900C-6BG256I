LOCATE COMP "green" SITE "J11" ;
LOCATE COMP "red_p" SITE "N15" ;
LOCATE COMP "green_p" SITE "P16" ;
LOCATE COMP "reset" SITE "N1" ;

IOBUF PORT "reset" PULLMODE=UP IO_TYPE=LVCMOS33 ;
IOBUF PORT "red" PULLMODE=UP IO_TYPE=LVCMOS33 ;
IOBUF PORT "yellow" PULLMODE=UP IO_TYPE=LVCMOS33 ;
IOBUF PORT "green" PULLMODE=UP IO_TYPE=LVCMOS33 ;
